<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/colorWhite" />
            <corners android:radius="50dp" />
        </shape>

    </item>

    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape>
            <corners android:radius="50dp" />
            <solid android:color="@color/colorGray" />
        </shape>
    </item>

</layer-list>